---
layout: null
---
{
  "posts" : [
    {% for post in site.posts %}
    {
    "title": "{{ post.title }}",
    "director": "{{post.director}}",
    "year": "{{post.year}}",
    "cover": "{{post.cover}}" ,
    "url": "{{post.url}}"
    }{% unless forloop.last %},{% endunless %}
    {% endfor %}
  ]
}